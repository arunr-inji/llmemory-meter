# LLMemoryMeter Configuration
# Copy this file to .env and add your actual API keys

# Mem0 API Configuration
MEM0_API_KEY=your_mem0_api_key_here

# OpenAI API Configuration  
OPENAI_API_KEY=your_openai_api_key_here

# Zep Memory API Configuration
ZEP_API_KEY=your_zep_api_key_here

# Optional: Additional memory tool APIs
LANGCHAIN_API_KEY=your_langchain_api_key_here

# Performance Settings
DEFAULT_TIMEOUT=30
MAX_RETRIES=3
DEBUG=false

# Example API Keys (for reference - get real keys from providers):
# MEM0_API_KEY=mem0_sk_1234567890abcdef
# OPENAI_API_KEY=sk-1234567890abcdef1234567890abcdef
